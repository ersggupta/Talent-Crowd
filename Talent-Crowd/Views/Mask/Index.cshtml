<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
<script type="text/javascript" src="http://www.proyectus.com/ChromeExt/bootstrap.min.js"></script>
<script type="text/javascript" src="http://www.proyectus.com/ChromeExt/popup.js"></script>

<form id="formZoho">
    <select id="Job_Request" name="Job_Request" class="form-control"></select>

    <h1>Basic Info</h1>
    <table>
        <tr><td>First Name</td><td><input type="text" class="form-control" name="First_Name" id="First_Name" placeholder="First Name"> </td></tr>
        <tr><td>Sur Name</td><td><input type="text" class="form-control" name="Sur_Name" id="Sur_Name" placeholder="Sur Name"></td></tr>
        <tr><td>Location</td><td><input type="text" class="form-control" name="Location" id="Location" placeholder="Location"></td></tr>
        <tr><td>Current Employeer</td><td><input type="text" class="form-control" name="Current_Employeer" id="Current_Employeer" placeholder="Current Employeer"></td></tr>
        <tr><td>Source Site</td><td><input type="text" class="form-control" name="Source_Site" id="Source_Site" placeholder="Source Site"></td></tr>
        <tr><td>Profile URL</td><td><input type="text" class="form-control" name="Profile_URL" id="Profile_URL" placeholder="Profile URL"></td></tr>
        <tr><td>DOB</td><td><input type="text" class="form-control" name="DOB" id="DOB" placeholder="DOB"></td></tr>
        <tr><td>Description of Current Work</td><td><input type="text" class="form-control" name="Description_of_current_work" id="Description_of_current_work" placeholder="Description of current work"></td></tr>
    </table>

    <h1>Skills</h1>
    <div style="width:50%;float:left">
        <h4>Primary Skills</h4>
        <div id="JobRequestSkillsP">
        </div>
    </div>
    <div style="width:50%;float:left">
        <h4>Secondary Skills</h4>
        <div id="JobRequestSkillsS">
        </div>
    </div>
</form>